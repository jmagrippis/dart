{
  "name": "digital-self-assistant",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "9.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.0",
    "eslint-plugin-import": "^2.0",
    "eslint-plugin-jsx-a11y": "^6.0",
    "eslint-plugin-react": "^7.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
